{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "max-len": [
            "error",
            {
                "code": 300
            }
        ],
        "import/extensions": ["warn", "always"]
    }
}
